@article{schmidCatalysingOrganocatalysisTrends2024,
  title = {Catalysing (Organo-)Catalysis: {{Trends}} in the Application of Machine Learning to Enantioselective Organocatalysis},
  shorttitle = {Catalysing (Organo-)Catalysis},
  author = {Schmid, Stefan P. and Schlosser, Leon and Glorius, Frank and Jorner, Kjell},
