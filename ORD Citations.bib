@article{baroReactionSpaceCharting2024,
  title = {Reaction {{Space Charting}} as a {{Tool}} in {{Organic Chemistry Research}} and {{Development}}},
  author = {Bar{\'o}, Eloy Lozano and Nadal Rodr{\'i}guez, Pau and {Ju{\'a}rez-Jim{\'e}nez}, Jordi and Ghashghaei, Ouldouz and Lavilla, Rodolfo},
  year = {2024},
  journal = {Advanced Synthesis \& Catalysis},
  volume = {366},
  number = {4},
  pages = {551--573},
  issn = {1615-4169},
  doi = {10.1002/adsc.202301205},
  urldate = {2025-05-01},
  abstract = {The chemical and reaction spaces are incredibly vast and special procedures are required to study them. Their complex and multiparametric nature encompass myriads of compounds and interactions and endless modifications involving the distinct impact of relevant variables (temperature, solvent, stoichiometry, etc.). This often calls for the design, collection, and analysis/interpretation of large datasets. Reaction charting, the systematic scanning, description, analysis, and guided modifications of a given process, stands as the most promising approach. It offers fast and accurate solutions as well as expanding the knowledge about the systems. This deeper understanding enables reliable predictions, improvement of the productivity (yields, scope), eventually leading to sustainable, economic, and safe applications. The present review introduces the topic, analyzing selected examples and recent advancements in different organic chemistry-related fields. The covered methodologies range from classical experimentation modifying one factor at a time, to Design of Experiments and more modern computational approaches involving Machine Learning and Artificial Intelligence. In this way, the impact of charting in process development, biological and medicinal chemistry, catalysis, reaction discovery and computational methods is accounted. Finally, the conclusion/outlook section gives a general appraisal and a prospect on the future implementation of the methodology.},
  copyright = {{\copyright} 2024 The Authors. Advanced Synthesis \& Catalysis published by Wiley-VCH GmbH},
  langid = {english},
  keywords = {Cites ORD,Computational chemistry,Medicinal chemistry,Molecular diversity,Reaction discovery,Synthesis design}
}

@misc{bashkyrtsevMeve2025,
  title = {Meve},
  author = {Bashkyrtsev, Stanislav},
  year = {2025},
  month = apr,
  urldate = {2025-05-02},
  abstract = {The idea behind Meve is to record Molecular Events. Could be anything: synthesized, purified, assayed, sketched - doesn't matter. What groups multiple events is a "context". For instance, a context could be a reaction - then you can find all the events within the context, which gives you all the inputs \& outputs of the reaction. Or a sketch ID, which could be your ideation page that involves 20 compounds - you'll be able to see 20 events together. And so on. This abstraction makes it universal - whoever owns Meve defines what an Event is, which events they are interested in, the granularity at which to group them (the context), etc. This allows anyone (be it a person or another software) find anything happening to a compound.},
  howpublished = {Elsci LLC},
  keywords = {cheminformatics,chemistry,drug discovery,materials science},
  note = {\href{https://www.linkedin.com/posts/elsci-io_drugdiscovery-materialscience-chemistry-activity-7307294750280179712-VztI?utm_source=li_share&utm_content=feedcontent&utm_medium=g_dt_web&utm_campaign=copy}{https://www.linkedin.com/posts/elsci-io\_drugdiscovery-materialscience-chemistry-activity-7307294750280179712-VztI?utm\_source=li\_share\&utm\_content=feedcontent\&utm\_medium=g\_dt\_web\&utm\_campaign=copy} is Elsci' LinkedIn announcement that Meve includes the imported ORD data.}
