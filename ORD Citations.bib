@misc{walshCRIPTScalablePolymer2022,
  title = {{{CRIPT}}: {{A Scalable Polymer Material Data Structure}}},
  shorttitle = {{{CRIPT}}},
  author = {Walsh, Dylan and Zou, Weizhong and Schneider, Ludwig and Mello, Reid and Deagen, Michael and Mysona, Joshua and Lin, Tzyy-Shyang and de Pablo, Juan and Jensen, Klavs and Audus, Debra and Olsen, Bradley},
  year = {2022},
  month = sep,
  publisher = {ChemRxiv},
  doi = {10.26434/chemrxiv-2022-xpz37},
  urldate = {2025-05-01},
  abstract = {Polymeric materials are integral components of nearly every aspect of modern life. Today, polymer scientists and engineers devote significant resources to the design and development of these materials to meet growing societal needs. However, developing cheminformatic solutions for polymers has been difficult since they are large stochastic molecules with hierarchical structures spanning multiple length scales from chemical bonds to large molecular assemblies. Here we present the design for a general material data model that underpins the Community Resource for Innovation in Polymer Technology (CRIPT) data ecosystem. Among the key challenges that the data model addresses are the high complexity in defining a polymer structure and the intricacies involved with characterizing material properties. The core design of the data model is graph-based which provides flexibility, robustness, and scalability to support the community-driven mission. This approach to structuring material data provides the key advancements that the community needs to bring cheminformatics to polymer science and accelerate the development of new materials.},
  archiveprefix = {ChemRxiv},
  langid = {english},
  keywords = {Cites ORD}
}

@article{wangGenericInterpretableReaction2023,
