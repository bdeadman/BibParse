@article{strieth-kalthoffArtificialIntelligenceRetrosynthetic2024,
  title = {Artificial {{Intelligence}} for {{Retrosynthetic Planning Needs Both Data}} and {{Expert Knowledge}}},
  author = {{Strieth-Kalthoff}, Felix and Szymku{\'c}, Sara and Molga, Karol and {Aspuru-Guzik}, Al{\'a}n and Glorius, Frank and Grzybowski, Bartosz A.},
  year = {2024},
  month = apr,
  journal = {J. Am. Chem. Soc.},
  volume = {146},
  number = {16},
  pages = {11005--11017},
  publisher = {American Chemical Society},
  issn = {0002-7863},
  doi = {10.1021/jacs.4c00338},
  urldate = {2025-05-01},
  abstract = {Rapid advancements in artificial intelligence (AI) have enabled breakthroughs across many scientific disciplines. In organic chemistry, the challenge of planning complex multistep chemical syntheses should conceptually be well-suited for AI. Yet, the development of AI synthesis planners trained solely on reaction-example-data has stagnated and is not on par with the performance of ``hybrid'' algorithms combining AI with expert knowledge. This Perspective examines possible causes of these shortcomings, extending beyond the established reasoning of insufficient quantities of reaction data. Drawing attention to the intricacies and data biases that are specific to the domain of synthetic chemistry, we advocate augmenting the unique capabilities of AI with the knowledge base and the reasoning strategies of domain experts. By actively involving synthetic chemists, who are the end users of any synthesis planning software, into the development process, we envision to bridge the gap between computer algorithms and the intricate nature of chemical synthesis.},
  keywords = {Cites ORD}
}

@article{strieth-kalthoffMachineLearningChemical2022,
  title = {Machine {{Learning}} for {{Chemical Reactivity}}: {{The Importance}} of {{Failed Experiments}}},
  shorttitle = {Machine {{Learning}} for {{Chemical Reactivity}}},
  author = {{Strieth-Kalthoff}, Felix and Sandfort, Frederik and K{\"u}hnemund, Marius and Sch{\"a}fer, Felix R. and Kuchen, Herbert and Glorius, Frank},
  year = {2022},
  journal = {Angewandte Chemie International Edition},
  volume = {61},
  number = {29},
  pages = {e202204647},
  issn = {1521-3773},
  doi = {10.1002/anie.202204647},
  urldate = {2025-05-01},
  abstract = {Assessing the outcomes of chemical reactions in a quantitative fashion has been a cornerstone across all synthetic disciplines. Classically approached through empirical optimization, data-driven modelling bears an enormous potential to streamline this process. However, such predictive models require significant quantities of high-quality data, the availability of which is limited: Main reasons for this include experimental errors and, importantly, human biases regarding experiment selection and result reporting. In a series of case studies, we investigate the impact of these biases for drawing general conclusions from chemical reaction data, revealing the utmost importance of ``negative'' examples. Eventually, case studies into data expansion approaches showcase directions to circumvent these limitations---and demonstrate perspectives towards a long-term data quality enhancement in chemistry.},
  copyright = {{\copyright} 2022 Wiley-VCH GmbH},
  langid = {english},
  keywords = {Cites ORD,Cross-Coupling,Data Bias,Machine Learning,Reaction Data,Yield Prediction}
}

@article{strieth-kalthoffMaschinellesLernenZur2022,
  title = {{Maschinelles Lernen zur Vorhersage chemischer Reaktivit{\"a}t: Die Bedeutung ``gescheiterter'' Experimente}},
  shorttitle = {{Maschinelles Lernen zur Vorhersage chemischer Reaktivit{\"a}t}},
  author = {{Strieth-Kalthoff}, Felix and Sandfort, Frederik and K{\"u}hnemund, Marius and Sch{\"a}fer, Felix R. and Kuchen, Herbert and Glorius, Frank},
  year = {2022},
  journal = {Angewandte Chemie},
  volume = {134},
  number = {29},
  pages = {e202204647},
  issn = {1521-3757},
  doi = {10.1002/ange.202204647},
  urldate = {2025-05-01},
  abstract = {Die quantitative Vorhersage der Ergebnisse chemischer Reaktionen ist eine zentrale Herausforderung in allen synthetischen Disziplinen. W{\"a}hrend die darauf aufbauenden Optimierungsprobleme klassischerweise empirisch adressiert werden, birgt die datengest{\"u}tzte Modellierung ein enormes Potential, diese Prozesse zu beschleunigen. F{\"u}r solche Vorhersagemodelle werden jedoch gro{\ss}e Mengen hochwertiger Daten ben{\"o}tigt, die nur begrenzt verf{\"u}gbar sind: Zu den Hauptgr{\"u}nden hierf{\"u}r geh{\"o}ren experimentelle Fehler sowie besonders die menschliche Voreingenommenheit hinsichtlich der Auswahl von Experimenten und der Ver{\"o}ffentlichung von Ergebnissen. In einer Reihe von Fallstudien untersuchen wir die Auswirkungen dieser Verzerrungen auf die Qualit{\"a}t allgemeiner Schlussfolgerungen aus chemischen Reaktionsdaten, wobei die gro{\ss}e Bedeutung von ``gescheiterten'' Beispielen deutlich wird. Schlie{\ss}lich zeigen Fallstudien zu Ans{\"a}tzen der Datenerweiterung Wege auf, wie diese Einschr{\"a}nkungen umgangen werden k{\"o}nnen - und demonstrieren somit Perspektiven f{\"u}r eine langfristige Verbesserung der Datenqualit{\"a}t in der Chemie.},
  copyright = {{\copyright} 2022 Wiley-VCH GmbH},
  langid = {ngerman},
  keywords = {Chemische Reaktionsdaten,Cites ORD,Kreuzkupplungen,Maschinelles Lernen,Verzerrte Daten,Vorhersage von Ausbeuten}
}

