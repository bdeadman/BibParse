  author = {Wang, Xiaorui and Hsieh, Chang-Yu and Yin, Xiaodan and Wang, Jike and Li, Yuquan and Deng, Yafeng and Jiang, Dejun and Wu, Zhenxing and Du, Hongyan and Chen, Hongming and Li, Yun and Liu, Huanxiang and Wang, Yuwei and Luo, Pei and Hou, Tingjun and Yao, Xiaojun},
